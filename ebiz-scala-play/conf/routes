
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


# ProductController
GET     /product                controllers.ProductController.getAllProducts()
GET     /product/:id                controllers.ProductController.getProductById(id: Int)
POST     /product                  controllers.ProductController.postProduct()
PUT     /product/:id                  controllers.ProductController.updateProductById(id:Int)
DELETE     /product/:id                  controllers.ProductController.deleteProductById(id:Int)

